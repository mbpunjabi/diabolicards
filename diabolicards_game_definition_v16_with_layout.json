{
  "zone_structure": {
    "zones": [
      "Vought Tower",
      "The Boys Hideout",
      "Godolkin Campus"
    ],
    "slots_per_zone": 8,
    "diabolical_slot_per_zone": 1,
    "se_card_placement_area": "around_diabolical_slot"
  },
  "game_rules": {
    "turn_order": {
      "start_condition": "random_player",
      "default_order": "clockwise",
      "modifiers": [
        "strategy_ability",
        "roulette_ability"
      ]
    },
    "placement_rules": {
      "slots": 8,
      "diabolical_slot": "reserved",
      "restrictions": {
        "SE_cards": "must be placed near diabolical slot",
        "diabolical_cards": "use diabolical slot only"
      }
    },
    "win_condition": {
      "trigger": "no_moves_left",
      "calculation": {
        "total_power": "sum of alive cards with all boosts/hinders applied",
        "synergy_bonus": {
          "applicable_groups": [
            "Godolkin Class",
            "Vought Executives",
            "The Boys"
          ],
          "per_card": 1,
          "zone_cap": 4
        }
      },
      "tie": true
    }
  },
  "event_rules": {
    "scorch_the_earth": {
      "trigger_condition": {
        "type": "sequence",
        "steps": [
          "one_side_uses_SE_to_kill_all_in_zone",
          "other_side_uses_SE_same_ability_next_turn_same_zone"
        ],
        "zone_must_be_fully_filled": true
      },
      "impact": {
        "remove_SE_cards": true,
        "seal_zone": true
      },
      "references": [
        "SE_card_usage"
      ]
    }
  },
  "state_definitions": {
    "how_dare_you": {
      "activation_condition": {
        "type": "hand_state",
        "condition": "player_has_both_SE_cards"
      },
      "effects": {
        "restrict_same_SE_ability_twice": true,
        "same_zone_double_SE_use": {
          "trigger": true,
          "impact": "both_SE_cards_die"
        }
      },
      "references": [
        "SE_card_logic"
      ]
    }
  },
